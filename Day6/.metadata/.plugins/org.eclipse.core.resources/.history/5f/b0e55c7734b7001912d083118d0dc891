package com.hcl.generics;

class Data{
	public void swap(int x, int y){
	    x = x + y;
	    y = x - y;
	    x = x - y;
	    System.out.println(" X : " + x + " y : " + y);
	  }
}
public class SwapDemo {
  
  /**
   * main.
   * @param args .
   */

  public static void main(String[] args) {
    Data obj = new Data();
    int x;
    int y;
    obj.swap(x , y);
}
}
