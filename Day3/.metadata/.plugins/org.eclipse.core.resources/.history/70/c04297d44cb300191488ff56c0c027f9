package com.hcl.varargs;

public class Attendence {

	public static void show(String... name){ // varying arguments
		for(String s: name ){
			System.out.print(s);
		}System.out.println();
	}
	public static void main(String[] args) {
		show();
		show("YASH");
		show("ANAND"," RISHAB");
		show("1","2","3","4","5");
	}
}
