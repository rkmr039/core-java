package com.hcl.abstractClass;

abstract class Training{
	abstract void name();
	abstract void email();
}

class Rishab extends Training{

	@Override
	void name() {
		System.out.println("Rishab...");
		
	}

	@Override
	void email() {
		System.out.println("rkmr039@gmail.com");
	}
	
}

public class AbsDemo{


}
