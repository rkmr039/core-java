package com.hcl.ems;

import java.util.Date;
import java.util.List;

class HolidayObject{
	int day;
	int month;
	HolidayClass(int day, int month){
		this.day = day;
		this.month = month;
	}
	
}
public class Holidays {

	public int getHolidays(Date startDate, Date enddate){
		int holidays = 0;
		
		List<HolidayClass> holidaysList = new ArrayList<>();
		
		
		return holidays;
		
		
	}
}
