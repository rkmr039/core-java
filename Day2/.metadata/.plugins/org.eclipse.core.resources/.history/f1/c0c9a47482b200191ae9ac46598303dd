package com.hcl.boxing;

public class BoxStudent {

	public void show(Object obj){
		Student e = (Student)obj;
		System.out.println(e);
	}
	public static void main(String[] args) {
		Student objEmploy = new Student();
		objEmploy.empno = 1;
		objEmploy.name="RISHAB";
		objEmploy.basic=15005;
		
		
		new BoxStudent().show(objEmploy);
	}
}
