package com.hcl.boxing;

public class StudentSearch {

	public Student searchStudent(int sno){
		Student obj = null;
		if(sno == 1){
			obj.sno = 10017826;
			obj.name = "RISHAB";
			obj.city = "Delhi";
			obj.cgp = 7.5;
		}
		if(sno==2){
			obj.sno = 1003455;
			obj.name = "YASH";
			obj.city = "BANGALORE";
			obj.cgp = 7.5;
		}
		return obj;
	}
	public static void main(String[] args) {
		System.out.println(new StudentSearch().searchStudent(1));
	}
}
