package com.hcl.boxing;

public class EmploySearch {

	public Employ showEmploy(int empno){
		Employ emp=null;
		
		if(empno == 1){
			emp = new Employ();
			emp.empno=1;
			emp.name="Anand";
			emp.basic=34567;
		}
		if(empno == 1){
			emp = new Employ();
			emp.empno=2;
			emp.name="Anand";
			emp.basic=34567;
		}
		
		return emp;
		
	}
	public static void main(String[] args) {
	
		
	}
}
