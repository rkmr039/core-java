public class Scan3 {

	public static void main(String[] args) {
		try {
			FileWriter fw = new FileWriter("C:/files.data.txt");
			fw.write("12 r t 45 45.56 true rishu");
			fw.close();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
