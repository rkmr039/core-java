package com.hcl.java;

public class EvenOdd {

	public void check(int n) {
		if (n >= 0) {
			System.out.println("Positive No...");
		} else {
			System.out.println("Negative No...");
		}
	}

	public static void main(String[] args) {
		int n = -12;
		EvenOdd obj = new EvenOdd();
		obj.check(n);

	}

}
